def basement(row):
    current_floor = 0

    for position, char in enumerate(row, 1):
        if char == 'a':
            current_floor += 1
        elif char == 'b':
            current_floor -= 1

        if current_floor == -1:
            return position

    return -1

row = "()"
position = basement(row)
print(f"The first time in basement is at {position-1}.")
